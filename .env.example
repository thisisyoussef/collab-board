# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Socket.IO Frontend Configuration
VITE_SOCKET_SERVER_URL=https://your-socket-server.onrender.com

# Anthropic Configuration (server-side only, for Vercel functions)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
ANTHROPIC_MODEL=claude-sonnet-4-20250514

# OpenAI Configuration (server-side only, for Vercel functions)
OPENAI_API_KEY=sk-proj-your-openai-key
OPENAI_MODEL=gpt-4.1-mini

# AI Provider Routing
# anthropic | openai | ab
AI_PROVIDER_MODE=anthropic
# Used only when AI_PROVIDER_MODE=ab
AI_OPENAI_PERCENT=50
# Allow request-level provider/model overrides (for benchmark automation only)
AI_ALLOW_EXPERIMENT_OVERRIDES=false

# LangSmith Tracing (server-side only)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=collab-board-dev
LANGCHAIN_API_KEY=lsv2_pt_your-langsmith-key
# Optional custom LangSmith API endpoint
# LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Benchmark Runner (optional, local or CI)
AI_BASE_URL=https://collab-board-iota.vercel.app
AI_AUTH_TOKEN=your-firebase-id-token
AB_BOARD_IDS=board-a,board-b
AB_AUTO_CREATE_BOARDS=0
AB_BOARD_PREFIX=ab-bench
AB_ROUNDS=2
AB_CONCURRENCY=4
AB_DELAY_MS=50
AB_TIMEOUT_MS=45000
AB_MAX_REQUESTS=0
AB_WAIT_READY=false
AB_READY_TIMEOUT_MS=600000
AB_READY_INTERVAL_MS=10000
AB_MODEL_MATRIX=anthropic:claude-sonnet-4-20250514,openai:gpt-4.1-mini,openai:gpt-4.1

# Socket.IO Server Environment (Render service)
SOCKET_CORS_ORIGIN=https://your-frontend-domain.vercel.app
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxx@your-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY=\"-----BEGIN PRIVATE KEY-----\\n...\\n-----END PRIVATE KEY-----\\n\"
FIREBASE_WEB_API_KEY=your-web-api-key
BENCHMARK_USER_ID=benchmark-user-uid

# Optional
VITE_ENABLE_METRICS=true
