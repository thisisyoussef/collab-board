# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Socket.IO Frontend Configuration
VITE_SOCKET_SERVER_URL=https://your-socket-server.onrender.com

# Anthropic Configuration (server-side only, for Vercel functions)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Socket.IO Server Environment (Render service)
SOCKET_CORS_ORIGIN=https://your-frontend-domain.vercel.app
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxx@your-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY=\"-----BEGIN PRIVATE KEY-----\\n...\\n-----END PRIVATE KEY-----\\n\"

# Optional
VITE_ENABLE_METRICS=true
